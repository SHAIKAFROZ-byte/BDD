<suite name="TestNG Suite" parallel="tests">
    <listeners>
        <listener class-name="Testng.TestListener"/>
    </listeners>
    <test name="TestNG">
        <groups>
            <run>
                <include name="mathOperations"/>
                <include name="negativeCases"/>
            </run>
        </groups>
        <classes>
            <class name="Testng.TestClass"/>
        </classes>
    </test>
</suite>
